#include<stdio.h>
int main()
{
    int a[10],n,i,min;
    printf("to print the minimum of the number");
    scanf("%d",&n);
    for(i=0;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
    min=a[0];
    for(i=0;i<=n;i++)
    {
        if(a[i]<min)
        min=a[i];
    }
    printf("the minimum number is an %d",min);
    return 0;
}
