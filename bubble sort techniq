#include<stdio.h>
int main()
{
	int a[10]={6,8,3,9,5,7,2,4,5,10,1};
	printf("to print the numbers in the bubble chart numbers wise");
	if(a[9]>a[3]||a[3]>a[1]||a[1]>a[5]||a[5]>a[0]||a[0]>a[4]||a[4]>a[7]||a[7]>a[2]||a[2]>a[6]||a[6]>a[10]||a[10]<a[9])
	{
		printf("%d %d %d %d",a[9],a[3],a[1],a[5]);
		printf("%d %d %d %d",a[0],a[4],a[7],a[2]);
		printf("%d %d",a[6],a[10]);
		printf("\n");
	}
	else 
	{
		printf("to print the numbers properly");
	}
	return 0;
}
