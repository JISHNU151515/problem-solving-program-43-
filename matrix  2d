#include<stdio.h>
int main()
{
    int a[10][10],row,col,i,j;
    printf("to print the matrix in the form of the dyananmic ");
    scanf("%d %d",&row,&col);
    for(i=0;i<=row;i++)
    {
        for(j=0;j<=col;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<=col;i++)
    {
        for(j=0;j<=col;j++)
        {
            printf("%d",a[i][j]);
        }
        printf("\n");
    }
    return 0;
}
